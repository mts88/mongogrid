{
    "name": "mts88/mongogrid",
    "description": "GridFS of MongoDB for Laravel",
    "license": "MIT",    
    "version": "1.1.0",
    "keywords": [
        "mongodb", "gridfs", "mongo", "grid", "laravel", "mongogrid", "storage", "file", "bucket", "database", "store", "documents"
    ],
    "type": "library",
    "require": {
        "mongodb/mongodb": "^1.3.0",
        "ramsey/uuid": "^3.7.0"
    },
    "authors": [
        {
            "name": "Francesca Motisi",
            "email": "fsca.motisi@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mts88\\MongoGrid\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mts88\\MongoGrid\\Providers\\MongoGridServiceProvider"
            ],
            "aliases": {
                "MongoGrid": "Mts88\\MongoGrid\\Facades\\MongoGrid"
            }
        }
    }
}
